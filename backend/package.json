{
  "name": "portfolio-backend",
  "version": "1.0.0",
  "description": "Backend API for Portfolio Admin System",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node scripts/setup.js",
    "seed-admin": "node utils/seedAdmin.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "echo 'No build step required for Node.js'",
    "clean": "rm -rf uploads/temp logs/*.log",
    "backup": "mongodump --uri $MONGODB_URI --out backup/$(date +%Y%m%d_%H%M%S)",
    "restore": "mongorestore --uri $MONGODB_URI --drop",
    "logs": "tail -f logs/portfolio.log",
    "health": "curl http://localhost:5000/api/health"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.8.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "portfolio",
    "admin",
    "api",
    "nodejs",
    "express",
    "mongodb",
    "jwt",
    "analytics"
  ],
  "author": "Portfolio Admin System",
  "license": "MIT"
}
